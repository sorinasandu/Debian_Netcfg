msgstr ""
" netkort         = ${interface}\n"
" maskinnavn      = ${hostname}\n"
" domæne          = ${domain}\n"
" IP-adresse      = ${ipaddress}\n"
" netmaske        = ${netmask}\n"
" gateway         = ${gateway}\n"
